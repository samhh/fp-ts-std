{
  "name": "fp-ts-std",
  "description": "The missing pseudo-standard library for fp-ts.",
  "version": "0.17.1",
  "license": "MIT",
  "author": "Sam A. Horvath-Hunt <hello@samhh.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/samhh/fp-ts-std"
  },
  "homepage": "https://github.com/samhh/fp-ts-std",
  "bugs": "https://github.com/samhh/fp-ts-std/issues",
  "keywords": [
    "functional-programming",
    "typescript",
    "fp-ts"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf ./dist/ && mkdir -p ./dist/esm/ ./dist/cjs/ && tsc -p ./tsconfig.build-esm.json && tsc -p ./tsconfig.build-cjs.json && tsc -p ./tsconfig.build-types.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src/ ./test/ --ext ts",
    "fmt": "prettier .",
    "test": "jest",
    "docs-ci": "docs-ts",
    "docs": "docs-ts && tshm-docs-ts ./docs/modules/*",
    "prepublish": "yarn run build"
  },
  "devDependencies": {
    "@swc/core": "^1.3.53",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.4.0",
    "@types/jsdom": "^21.1.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "docs-ts": "^0.7.2",
    "eslint": "^8.0.1",
    "eslint-plugin-functional": "^5.0.8",
    "fast-check": "^3.1.2",
    "fp-ts": "^2.16.0",
    "fp-ts-laws": "^0.3.0",
    "jest": "^29.4.1",
    "jsdom": "^21.1.1",
    "monocle-ts": "^2.3.0",
    "newtype-ts": "^0.3.0",
    "prettier": "^2.1.2",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "fp-ts": "^2.16.0",
    "monocle-ts": "^2.3.0",
    "newtype-ts": "^0.3.0"
  }
}
