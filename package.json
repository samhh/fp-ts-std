{
  "private": true,
  "name": "fp-ts-std",
  "description": "The missing pseudo-standard library for fp-ts.",
  "version": "0.9.0",
  "license": "MIT",
  "author": "Sam A. Horvath-Hunt <hello@samhh.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/samhh/fp-ts-std"
  },
  "homepage": "https://github.com/samhh/fp-ts-std",
  "bugs": "https://github.com/samhh/fp-ts-std/issues",
  "keywords": [
    "functional-programming",
    "typescript",
    "fp-ts"
  ],
  "main": "./index.js",
  "sideEffects": false,
  "scripts": {
    "dev": "tsc -p ./tsconfig.build.json -w",
    "build": "tsc -p ./tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "typecheck-dev": "tsc -w --noEmit",
    "lint": "eslint ./src/ ./test/ --ext ts",
    "fmt": "prettier --write .",
    "fmt-check": "prettier --check .",
    "test": "jest",
    "docs": "docs-ts",
    "docs-tshm": "tshm-docs-ts ./docs/modules/*",
    "prepub": "rm -rf dist && mkdir -p dist && cp ./README.md ./dist/ && cat ./package.json | grep -v '\"private\":' > ./dist/package.json && yarn run build"
  },
  "devDependencies": {
    "@types/eslint": "^7.0.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "docs-ts": "^0.6.7",
    "eslint": "^7.0.0",
    "eslint-plugin-functional": "3.1.0",
    "fast-check": "^2.5.0",
    "fp-ts": "^2.9.0",
    "jest": "^26.6.0",
    "monocle-ts": "^2.0.0",
    "newtype-ts": "^0.3.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "fp-ts": "^2.9.0",
    "monocle-ts": "^2.0.0",
    "newtype-ts": "^0.3.0"
  }
}
